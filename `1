class solution:
    def LeapYearCheck(y):
        if y % 4 == 0:
            if y % 100 == 0:
                if y % 400 == 0:
                    return True
                else:
                    return False
            else:
                return True
        else:
            return False
year = int(input("Enter the Year to be checked: "))
s=solution
if s.LeapYearCheck(year):
    print("Leap Year")
else:
    print("Not a Leap Year")
