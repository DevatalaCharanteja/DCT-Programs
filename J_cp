# DEVATALA CHARAN TEJA   [MCSAIR   ROLLNO:37   ADCKKD]

def cPN(n):
    c = 0
    for num in range(n):
        if num <= 1:
            continue
        for i in range(2, num):
            if num % i == 0:
                break
        else:
            c += 1
    return c


print(cPN(1))
print(cPN(100))
